<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<Pane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <VBox prefHeight="400.0" prefWidth="518.0" spacing="5.0" VBox.vgrow="ALWAYS">
      <Label fx:id="name" style="-fx-text-fill: WHITE;" styleClass="cell_big_label" text="\$name" />
      <FlowPane fx:id="tags" />
      <Label fx:id="phone" style="-fx-text-fill: WHITE;" styleClass="cell_small_label" text="\\$phone" />
      <Label fx:id="address" style="-fx-text-fill: WHITE;" styleClass="cell_small_label" text="\$address" />
      <Label fx:id="email" style="-fx-text-fill: WHITE;" styleClass="cell_small_label" text="\$email" />
      <VBox fx:id="class_list" prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS">
         <Label style="-fx-text-fill: WHITE;" styleClass="cell_big_label" text="Assigned Lessons:" />
         <StackPane prefHeight="150.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
            <ListView fx:id="assignedLessonList" prefHeight="200.0" prefWidth="200.0" />
         </StackPane>
      </VBox>
   </VBox>
</Pane>
